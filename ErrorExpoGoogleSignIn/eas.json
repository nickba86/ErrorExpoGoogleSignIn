   },
   "android_release_aab": {
     "android": {
       "buildType": "app-bundle",
       "developmentClient": false,
       "distribution": "store",
       "autoIncrement": true
     }
   },
   "ios_testflight": {
     "ios": {
       "developmentClient": true,
       "distribution": "internal",
       "autoIncrement": true
     }
   },
   "ios_release": {
     "ios": {
       "developmentClient": false,
       "distribution": "store",
       "autoIncrement": true
     }
   }
 }
}
